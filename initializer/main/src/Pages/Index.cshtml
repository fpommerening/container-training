@page
@model FP.ContainerTraining.Initializer.Main.Pages.IndexModel
@{
    ViewData["Title"] = "Systemstatus";
}

<div class="row p-3">
    <div class="col-12 text-center">
        <h1>Systemstatus</h1>
    </div>
</div>
<div class="row">
    <div class="col-3"></div>
    <div class="col-6">
        <table class="table table-striped">
            <thead>
            <tr>
                <th scope="col">Nummer</th>
                <th scope="col">Erstellt am</th>
            </tr>
            </thead>
            @{
                foreach (var version in @Model.Versions)
                {
                    <tr>
                        <td>@version.Number</td>
                        <td>@version.CreatedOn.ToString("G")</td>
                    </tr>
                }
            }
        </table>
    </div>
    <div class="col-3"></div>
</div>
  

